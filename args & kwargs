## use args function ##

def funargs(*args):
    
    for item in args:
        print(item)
    
har = ["harry", "Rohan", "skillf", "Hammad"]
funargs(*har)

## use normal function ##

def funargs(normal,*args):
    print(normal)
    for item in args:
        print(item)
    
har = ["harry", "Rohan", "skillf", "Hammad"]
normal = "This is normal"
funargs(normal,*har)

## use kwarge function ##

def funargs(normal,*args, **kwargs):
    print(normal)
    for item in args:
        print(item)
    for key, value in kwargs.items():
        print(key,value)
        
har = ["harry", "Rohan", "skillf", "Hammad"]
normal = "This is normal"
kw = {"rohan" : "monitor", "sanjay" : "engineer", "ruchi" : "doctor" }
funargs(normal,*har, **kw)



